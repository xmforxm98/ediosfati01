{
  "buildCommand": "echo 'Using static files'",
  "outputDirectory": "docs",
  "installCommand": "echo 'No install needed'",
  "framework": null,
  "functions": {},
  "routes": [
    {
      "src": "/privacy-policy",
      "dest": "/privacy-policy.html"
    },
    {
      "src": "/account-deletion",
      "dest": "/account-deletion.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/account-deletion",
      "destination": "/account-deletion.html"
    },
    {
      "source": "/privacy-policy", 
      "destination": "/privacy-policy.html"
    }
  ]
} 